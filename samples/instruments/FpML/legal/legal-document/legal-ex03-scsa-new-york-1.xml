                <debtType debtTypeScheme="http://www.fpml.org/coding-scheme/external/debt-type/standard-and-poors">Deposit</debtType>
              </debt>
            </creditNotation>
          </minimumRating>
        </custodianTerms>
      </eligibilityToHoldCollateral>
